<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.netjob.basicmusicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeScreenActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PodcastLibraryActivity"
                  android:label="@string/label_podcast_library_activity"/>
        <activity android:name=".MusicLibraryActivity"
                  android:label="@string/label_music_library_activity"/>
        <activity android:name=".EQActivity"
                  android:label="@string/label_eq_activity"></activity>
    </application>

</manifest>